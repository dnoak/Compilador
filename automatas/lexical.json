{
    "reserved_symbols_2char": {
        "start": ["$!=", "$<=", "$>=", "$:="]
    },
    "reserved_symbols_1char": {
        "start": ["$+", "$-", "$*", "$/", "$,", "$:", "$;", "$,", "$(", "$)", "${", "$}", "$>", "$<"]
    },
    "reserved_words": {
      "start": ["$program", "$var", "$procedure", "$if","$then","$else","$while","$do","$end","$read","$write", "$begin", "$integer", "$real"]
    }
}   